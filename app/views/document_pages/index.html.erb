<h1>Listing document_pages</h1>

<table>
  <thead>
    <tr>
      <th>Document</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @document_pages.each do |document_page| %>
      <tr>
        <td><%= document_page.document_id %></td>
        <td><%= document_page.image %></td>
        <td><%= link_to 'Show', document_page %></td>
        <td><%= link_to 'Edit', edit_document_page_path(document_page) %></td>
        <td><%= link_to 'Destroy', document_page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Document page', new_document_page_path %>
